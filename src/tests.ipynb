    "import os\n",
    "import inspect\n",
    "\n",
    "def dir_path():\n",
    "    # Gets the directory where this function is called from\n",
    "    frame = inspect.currentframe()\n",
    "    filename = frame.f_code.co_filename\n",
    "    return os.path.dirname(os.path.abspath(filename))"
